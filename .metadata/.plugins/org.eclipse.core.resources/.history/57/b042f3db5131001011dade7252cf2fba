package jp.co.attendance.Service;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import jp.co.attendance.Domain.Employee;
import jp.co.attendance.Repository.AttendanceRepository;

@Service
public class AttendanceService {
	@Autowired
	AttendanceRepository attendanceRepository;

	public void handleSave(Employee employee) {
		this.attendanceRepository.save(employee);
	}
}
